package com.aviva.documentroute;

import java.lang.Number;

rule "test2"
	dialect "mvel"
	when
		DocTrxGroup( name == "MAIN-DOCUMENT-FOR-PS" )
		( CheckList( name == "CONTRACT_CREATED" , value == "NO" ) )
	then
		DocumentRoute fact0 = new DocumentRoute();
		fact0.setReturnReason( "SC" );
		insert( fact0 );
end
